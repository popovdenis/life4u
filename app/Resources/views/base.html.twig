<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width"/>
	<!--[if IE]>
	<script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
	<title>{% block title %}LifeSafe4U{% endblock %}</title>
	<meta name="keywords" content=""/>
	<meta name="description" content=""/>
	{% block stylesheets %}
		<link rel="stylesheet" href="{{ asset('css/style.css') }}" type="text/css" media="all"/>
		<link rel="stylesheet" href="{{ asset('css/grid.css') }}" type="text/css" media="all"/>
		<link rel="stylesheet" href="{{ asset('css/base.css') }}" type="text/css" media="all"/>
		<link rel="stylesheet" href="{{ asset('css/buttons.css') }}" type="text/css" media="all"/>
		<link rel="stylesheet" href="{{ asset('css/icons.css') }}" type="text/css" media="all"/>
		<link rel="stylesheet" href="{{ asset('css/multi-select.css') }}" type="text/css" media="all"/>
	{% endblock %}
	{% block javascripts %}
		<script src="{{ asset('js/jquery.min.js') }}"></script>
		<script src="{{ asset('js/jquery.easing.min.js') }}"></script>
		<script src="{{ asset('js/jquery.multi-select.js') }}"></script>
		<script src="{{ asset('js/main.js') }}"></script>
		<script src="{{ asset('js/popup.js') }}"></script>
	{% endblock %}
	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
	<div id="wrapper">
		<header id="header">
			<h1 class="logo"><a title="LifeSafe4U" href="#">LifeSafe4U</a></h1>
			<div class="h-panel">
				<div class="h-left">
				</div>
				<div class="h-right">
				</div>
				<div class="clear">&nbsp;</div>
			</div>
		</header>
		<div id="middle">
			<div id="container">
				<div id="content">
					{% block content %}{% endblock %}
				</div>
			</div>
			<aside id="sideLeft">
			</aside>
		</div>
		<footer id="footer">&nbsp;</footer>
	</div>
</body>
</html>
